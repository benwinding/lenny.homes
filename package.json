{
  "name": "lenny.homes",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "rm -rf dist* .next && next build && yarn export",
    "export": "next-sitemap && next export -o dist-export && yarn prettier -w ./dist-export/*.html",
    "deploy": "yarn build && gh-pages -d dist-export --dotfiles",
    "start": "next dev",
    "clean": "rm -rf out",
    "lint": "next lint"
  },
  "dependencies": {
    "autoprefixer": "^10.4.0",
    "moment": "^2.29.1",
    "next": "12.0.1",
    "postcss": "^8.3.11",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tailwindcss": "^2.2.19"
  },
  "devDependencies": {
    "@types/node": "16.11.6",
    "@types/react": "17.0.33",
    "eslint": "7",
    "eslint-config-next": "12.0.1",
    "gh-pages": "^3.2.3",
    "next-sitemap": "^1.6.203",
    "prettier": "^2.2.1",
    "typescript": "4.4.4"
  }
}
